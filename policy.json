{
    "Version":"2012-10-17",
    "Statement":[
       {
          "Principal":{
             "AWS":"arn:aws:iam::034146468059:role/admin_role"
          },
          "Effect":"Allow",
          "Action":[
             "s3:GetObject",
             "s3:GetObjectVersion"
          ],
          "Resource":"arn:aws:s3:::mytest9060/*",
          "Condition":{
             "StringEquals":{
                "s3:ExistingObjectTag/environment":"production"
             }
          }
       }
    ]
 }